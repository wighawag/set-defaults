{
	"name": "set-defaults",
	"version": "0.0.2",
	"description": "copy default files to their actual file name",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"set-defaults": "dist/cli.cjs"
	},
	"devDependencies": {
		"@types/node": "^18.15.5",
		"rimraf": "^4.4.1",
		"tsup": "^6.7.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"commander": "^10.0.0"
	},
	"scripts": {
		"build": "rimraf dist && tsup --entry src/index.ts --entry src/cli.ts --dts --format esm,cjs"
	}
}
